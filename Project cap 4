#include<iostream>
using namespace std;
int main()
{
	int n,array[6];
	cout<<"ente the number of elements of array";//let us consider it as 5
	cin>>n;
	cout<<"enter the elements of array";
	for(int i=0;i<n;i++)
	{
		cin>>array[i];
	}
	cout<<"elements of array are : ";
	for(int i=0;i<n;i++)
	{
		cout<<array[i]<<endl;
	}
	return 0;
}
